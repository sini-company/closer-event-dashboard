          "format": "time_series",
          "rawSql": "WITH fullfill AS (\n  SELECT\n    DATE_TRUNC('$GroupBy', generate_series) AS time,\n    0 AS user_message,\n    0 AS bot_message,\n    0 AS agent_message\n  FROM GENERATE_SERIES(\n    $__timeFrom()::DATE,\n    $__timeTo()::DATE,\n    '1 $GroupBy'\n  )\n),\nresults AS (\n  SELECT\n    DATE_TRUNC('$GroupBy', timestamp) AS time,\n    COUNT(CASE WHEN event = 'bot.message.received' THEN 1 END) AS user_message,\n    COUNT(CASE WHEN event = 'bot.message.sent' AND data#>>'{message, meta, persona}' IS NULL THEN 1 END) AS bot_message,\n    COUNT(CASE WHEN event = 'bot.message.sent' AND data#>>'{message, meta, persona}' IS NOT NULL THEN 1 END) AS agent_message\n  FROM\n    events\n  WHERE\n    $__timeFilter(timestamp)\n    AND event IN ('bot.message.received', 'bot.message.sent')\n  GROUP BY time\n)\nSELECT\n  COALESCE(r.time, f.time) AS time,\n  COALESCE(r.user_message, f.user_message) AS \"사용자 메시지\",\n  COALESCE(r.bot_message, f.bot_message) AS \"봇 메시지\",\n  COALESCE(r.agent_message, f.agent_message) AS \"상담원 메시지\"\nFROM fullfill f\n  LEFT JOIN results r ON f.time = r.time\n",
          "refId": "A"
        }
